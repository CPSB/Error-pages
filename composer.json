{
    "name": "Tjoosten/ErrorPages",
    "type": "library",
    "description": "Our error pages in Laravel applications",
    "keywords": [
        "Tjoosten",
        "ErrorPages"
    ],
    "homepage": "https://github.com/Tjoosten/ErrorPages",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Joosten",
            "email": "Topairy@gmail.com",
            "homepage": "https://github.com/Tjoosten",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "illuminate/Support": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "ActivismeBE\\ErrorPages\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActivismeBE\\ErrorPages\\": "tests"
        }
    },

    "scripts": {
        "test": "phpunit",
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "ActivismeBe\\ErrorPages\\ProvidersServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
